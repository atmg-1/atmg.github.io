  z-index: 9999;
}
.particle {
  position: absolute;
  width: 6px;
  height: 6px;
  border-radius: 50%;
  pointer-events: none;
  animation: particleExplode 0.6s ease-out forwards;
}
@keyframes particleExplode {
  0% {
    transform: translate(0, 0);
