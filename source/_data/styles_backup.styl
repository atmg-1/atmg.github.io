// 楂樼骇闇撹櫣杈规鏁堟灉
.advanced-neon {
  position: relative;
  border: 2px solid;
  border-image: linear-gradient(45deg, #00ffff, #ff00ff, #00ffff, #ff00ff) 1;
  box-shadow: 
    0 0 5px rgba(0, 255, 255, 0.5),
    0 0 10px rgba(255, 0, 255, 0.3),
    0 0 15px rgba(0, 255, 255, 0.2),
    0 0 20px rgba(255, 0, 255, 0.1);
  animation: 
    neon-pulse 2s infinite alternate,
    border-flow 3s linear infinite;
}
@keyframes border-flow {
  0% { filter: hue-rotate(0deg); }
  100% { filter: hue-rotate(360deg); }
}
// 鍏ㄦ伅鎶曞奖鏁堟灉
.holographic-effect {
  position: relative;
  overflow: hidden;
}
.holographic-effect::before {
  content: '';
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  background: 
    repeating-linear-gradient(
      0deg,
      transparent,
      transparent 2px,
      rgba(0, 255, 255, 0.1) 2px,
      rgba(0, 255, 255, 0.1) 4px
    ),
    repeating-linear-gradient(
      90deg,
      transparent,
      transparent 2px,
      rgba(255, 0, 255, 0.1) 2px,
      rgba(255, 0, 255, 0.1) 4px
    );
  animation: 
    holographic-rotate 6s linear infinite,
    holographic-flicker 3s ease-in-out infinite;
  z-index: -1;
}
@keyframes holographic-rotate {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}
@keyframes holographic-flicker {
  0%, 100% { opacity: 0.3; }
  50% { opacity: 0.6; }
}
// 楂樼骇鏁呴殰鏁堟灉
.glitch-advanced {
  position: relative;
  overflow: hidden;
}
.glitch-advanced::before,
.glitch-advanced::after {
  content: attr(data-text);
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.glitch-advanced::before {
  color: rgba(0, 255, 255, 0.6);
  clip-path: polygon(0 0, 100% 0, 100% 45%, 0 45%);
  animation: 
    glitch-anim 0.3s infinite,
    offset-shift-top 6s infinite;
}
.glitch-advanced::after {
  color: rgba(255, 0, 255, 0.6);
  clip-path: polygon(0 55%, 100% 55%, 100% 100%, 0 100%);
  animation: 
    glitch-anim2 0.3s infinite,
    offset-shift-bottom 6s infinite;
}
@keyframes offset-shift-top {
  0%, 100% { transform: translate(0); }
  25% { transform: translate(-5px, -2px); }
  50% { transform: translate(3px, 2px); }
  75% { transform: translate(-3px, -1px); }
}
@keyframes offset-shift-bottom {
  0%, 100% { transform: translate(0); }
  25% { transform: translate(5px, 2px); }
  50% { transform: translate(-3px, -2px); }
  75% { transform: translate(3px, 1px); }
}
// 闇撹櫣鎵弿绾?
.neon-scanline {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 4px;
  background: linear-gradient(
    to bottom,
    rgba(0, 255, 255, 0.8),
    rgba(255, 0, 255, 0.8),
    transparent
  );
  box-shadow: 
    0 0 10px rgba(0, 255, 255, 0.8),
